# Synodos 환경 설정
# 이 파일을 .env로 복사해서 사용하세요
# cp .env.example .env

# DB 설정
DB_USERNAME=flow
DB_PASSWORD=flow123

# JWT 시크릿
JWT_SECRET=SynodosProjectSecretKeyForJWTTokenGeneration2024VeryLongSecretKey

# 프론트엔드 API 설정
API_URL=
WS_URL=/ws

# SMTP 설정 (Gmail 앱 비밀번호)
# Gmail에서 2단계 인증 후 앱 비밀번호 생성 필요
# https://myaccount.google.com/apppasswords
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Gemini AI API 설정
# https://aistudio.google.com/app/apikey 에서 API 키 생성
GEMINI_API_KEY=your-gemini-api-key

# GitHub OAuth 설정
# https://github.com/settings/developers 에서 OAuth App 생성
GITHUB_OAUTH_CLIENT_ID=your-github-client-id
GITHUB_OAUTH_CLIENT_SECRET=your-github-client-secret

# Google OAuth 설정
# https://console.cloud.google.com 에서 OAuth 2.0 클라이언트 ID 생성
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Naver OAuth 설정
# https://developers.naver.com 에서 애플리케이션 등록
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret

# Kakao OAuth 설정
# https://developers.kakao.com 에서 애플리케이션 등록
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret
